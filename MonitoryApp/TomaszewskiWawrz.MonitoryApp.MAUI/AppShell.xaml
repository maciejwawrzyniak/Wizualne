<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="TomaszewskiWawrz.MonitoryApp.MAUI.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:TomaszewskiWawrz.MonitoryApp.MAUI"
    Shell.FlyoutBehavior="Disabled"
    Title="TomaszewskiWawrz.MonitoryApp.MAUI">

    <FlyoutItem>
        <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
        <ShellContent
        Title="Monitory"
        ContentTemplate="{DataTemplate local:MonitoryPage}"
        Route="MonitoryPage" />

    </FlyoutItem>

</Shell>
